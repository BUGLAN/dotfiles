{
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "explorer.confirmDelete": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.mypy_cache": true,
        "**/.pytest_cahce": true,
        "**/.vscode": true,
        "**/*.py[cod]": true,
        "**/node_modules": true
    },
    "editor.fontFamily": "Monaco, Monaco, 'Courier New', monospace",
    "files.autoSave": "afterDelay",
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 1,
    "workbench.startupEditor": "newUntitledFile",
    "extensions.ignoreRecommendations": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "H",
            ],
            "after": [
                "^",
            ]
        },
        {
            "before": [
                "L",
            ],
            "after": [
                "$",
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "<leader>",
                "<space>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
    ],
    "vim.leader": ",",
}
