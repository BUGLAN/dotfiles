{
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [{
    "before": ["<leader>", "w"],
    "after": ["<Esc>"]
  }],
  "vim.normalModeKeyBindingsNonRecursive": [{
    "before": ["H"],
    "after": ["^"]
  },
  {
    "before": ["L"],
    "after": ["$"]
  },
  {
    "before": ["<tab>"],
    "after": ["%"]
  },

  {
    "before": ["<leader>", "<space>"],
    "commands": [":nohl"]
  },
  {
    "before": [";"],
    "after": [":"]
  },

  ],
  "vim.leader": ",",
}
