{
    "files.autoSave": "afterDelay",
    "workbench.colorTheme": "Palenight Theme",
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDelete": false,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "w"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "after": [
                "(",
                ")"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "after": [
                "{",
                "}"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "="
            ]
        },
        {
            "before": [
                "<leader>",
                "a"
            ],
            "after": [
                "+"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "-"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "<Esc>",
                "A",
                ":",
                "<Esc>",
                "o"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "H",
            ],
            "after": [
                "^",
            ]
        },
        {
            "before": [
                "L",
            ],
            "after": [
                "$",
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "<leader>",
                "<space>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "A",
                ":",
                "<Esc>",
                "o"
            ]
        }
    ],
    "vim.leader": ",",
}
